            "required": true,
            "schema": {
              "$ref": "#/definitions/RegisterModel"
            }
          }
        ],
        "responses": {}
      }
    },
    "/auth/active": {
      "post": {
        "tags": [
          "Auth"
        ],
        "description": "",
        "parameters": [
          {
            "name": "obj",
            "in": "headers",
            "required": true,
            "schema": {
              "$ref": "#/definitions/bearer"
            }
          }
        ],
        "responses": {}
      }
    }
  },
  "definitions": {
    "RegisterModel": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "example": "Ashok Sahu"
        },
        "account": {
          "type": "string",
          "example": "kanhasahu955902@gmail.com"
        },
        "password": {
          "type": "string",
          "example": "kanhasahu#1111"
        }
      },
      "required": [
        "name",
        "account",
        "password"
      ]
    }
  }
